@article{donald1980,
  title = {Correlations {{Between Brook Trout Growth}} and {{Environmental}}, {{Variables}} for {{Mountain Lakes}} in {{Alberta}}},
  author = {Donald, D B and Anderson, R S and Mayhood, D. W.},
  year = {1980},
  journal = {Transactions of the American Fisheries Society},
