@article{kozel1989,
  title = {Factors Influencing the Abundance of Brook Trout ({{Salvelinus}} Fonfinalis) in Forested Mountain Streams},
  author = {Kozel, Steven J. and Hubert, Wayne A.},
  year = {1989},
  journal = {Journal of Freshwater Ecology},
  volume = {5},
  number = {1},
  pages = {113--122},
  publisher = {{Taylor \& Francis Group}},
  doi = {10.1080/02705060.1989.9665219},
  abstract = {Physical and biological factors that appear to influence standing stocks (kg/ha) of brook trout (Salvelinus fontinalis) were identified in the Medicine Bow National Forest, Wyoming, for 32 forested reaches of mountain streams in drainages unaltered by human activity. Brook trout abundance declined as stream size increased. This decline appeared to be related to at least two factors\textemdash decline in habitat quality with increasing stream size and interaction with brown trout (Salmo trutta) at lower elevations. The greatest variation in brook trout abundance (R2=0.77) was accounted for by drainage basin area and elevation of the stream reach. \textcopyright{} Taylor \& Francis Group, LLC.}
}

@article{kudo1999,
  title = {Leptomycin {{B}} Inactivates {{CRM1}}/Exportin 1 by Covalent Modification at a Cysteine Residue in the Central Conserved Region},
  author = {Kudo, Nobuaki and Matsumori, Nobuaki and Taoka, Hiroshi and Fujiwara, Daisuke and Schreiner, Erwin P. and Wolff, Barbara and Yoshida, Minoru and Horinouchi, Sueharu},
  year = {1999},
  month = aug,
  journal = {Proceedings of the National Academy of Sciences of the United States of America},
  volume = {96},
  number = {16},
  pages = {9112--9117},
  issn = {0027-8424},
  abstract = {The cellular target of leptomycin B (LMB), a nuclear export inhibitor, has been identified as CRM1 (exportin 1), an evolutionarily conserved receptor for the nuclear export signal of proteins. However, the mechanism by which LMB inhibits CRM1 still remains unclear. CRM1 in a Schizosaccharomyces pombe mutant showing extremely high resistance to LMB had a single amino acid replacement at Cys-529 with Ser. The mutant gene, named crm1-K1, conferred LMB resistance on wild-type S. pombe, and Crm1-K1 no longer bound biotinylated LMB. 1H NMR analysis showed that LMB bound N-acetyl-l-cysteine methyl ester through a Michael-type addition, consistent with the idea that LMB binds covalently via its {$\alpha$},{$\beta$}-unsaturated {$\delta$}-lactone to the sulfhydryl group of Cys-529. When HeLa cells were cultured with biotinylated LMB, the only cellular protein bound covalently was CRM1. Inhibition by N-ethylmaleimide (NEM), an alkylating agent, of CRM1-mediated nuclear export probably was caused by covalent binding of the electrophilic structure in NEM to the sulfhydryl group of Cys-529, because the crm1-K1 mutant showed the normal rate for the export of Rev nuclear export signal-bearing proteins in the presence of not only LMB but also NEM. These results show that the single cysteine residue determines LMB sensitivity and is selectively alkylated by LMB, leading to CRM1 inactivation.},
  pmcid = {PMC17741},
  pmid = {10430904},
  file = {/Users/georgepv/Zotero/storage/2VNFJZM6/Kudo et al. - 1999 - Leptomycin B inactivates CRM1exportin 1 by covale.pdf;/Users/georgepv/Zotero/storage/4XHM2KBL/Kudo et al. - 1999 - Leptomycin B inactivates CRM1exportin 1 by covale.pdf;/Users/georgepv/Zotero/storage/M4M6QEJF/Kudo et al. - 1999 - Leptomycin B inactivates CRM1exportin 1 by covale.pdf}
}

@article{kunkel2003,
  title = {North {{American Trends}} in {{Extreme Precipitation}}},
