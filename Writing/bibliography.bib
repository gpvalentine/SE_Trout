  author = {Gregory, Richard D. and Willis, Stephen G. and Jiguet, Fr{\'e}d{\'e}ric and Vo{\v r}{\'i}{\v s}ek, Petr and Klva{\v n}ov{\'a}, Alena and van Strien, Arco and Huntley, Brian and Collingham, Yvonne C. and Couvet, Denis and Green, Rhys E.},
