@article{crane-droesch2013,
  title = {Heterogeneous Global Crop Yield Response to Biochar: A Meta-Regression Analysis},
  shorttitle = {Heterogeneous Global Crop Yield Response to Biochar},
  author = {{Crane-Droesch}, Andrew and Abiven, Samuel and Jeffery, Simon and Torn, Margaret S.},
  year = {2013},
  journal = {Environmental Research Letters},
  volume = {8},
  number = {4},
  pages = {044049},
  issn = {1748-9326},
  doi = {10.1088/1748-9326/8/4/044049},
  abstract = {Biochar may contribute to climate change mitigation at negative cost by sequestering photosynthetically fixed carbon in soil while increasing crop yields. The magnitude of biochar's potential in this regard will depend on crop yield benefits, which have not been well-characterized across different soils and biochars. Using data from 84 studies, we employ meta-analytical, missing data, and semiparametric statistical methods to explain heterogeneity in crop yield responses across different soils, biochars, and agricultural management factors, and then estimate potential changes in yield across different soil environments globally. We find that soil cation exchange capacity and organic carbon were strong predictors of yield response, with low cation exchange and low carbon associated with positive response. We also find that yield response increases over time since initial application, compared to non-biochar controls. High reported soil clay content and low soil pH were weaker predictors of higher yield response. No biochar parameters in our dataset\textemdash biochar pH, percentage carbon content, or temperature of pyrolysis\textemdash were significant predictors of yield impacts. Projecting our fitted model onto a global soil database, we find the largest potential increases in areas with highly weathered soils, such as those characterizing much of the humid tropics. Richer soils characterizing much of the world's important agricultural areas appear to be less likely to benefit from biochar.},
  langid = {english},
  file = {/Users/georgepv/Zotero/storage/8669NJA9/Crane-Droesch et al. - 2013 - Heterogeneous global crop yield response to biocha.pdf;/Users/georgepv/Zotero/storage/8Z3MTVU8/Crane-Droesch et al. - 2013 - Heterogeneous global crop yield response to biocha.pdf;/Users/georgepv/Zotero/storage/H9I2S275/Crane-Droesch et al. - 2013 - Heterogeneous global crop yield response to biocha.pdf}
}

@article{crane2017,
  title = {Trends in Body Condition of Smallmouth Bass and Northern Pike (1982\textendash 2013) Following Multiple Ecological Perturbations in the {{St}}. {{Lawrence River}}},
  author = {Crane, Derek P. and Farrell, John M.},
  year = {2017},
  journal = {Canadian Journal of Fisheries and Aquatic Sciences},
  volume = {74},
  number = {8},
  pages = {1158--1172},
  doi = {10.1139/cjfas-2016-0160},
  abstract = {Body condition indices can be used to investigate fish response to biological or environmental change. We used quantile regression to analyze a 32-year weight\textendash length data set (1982\textendash 2013) for smallmouth bass (Micropterus dolomieu) and northern pike (Esox lucius) from the St. Lawrence River to examine changes in condition (weight-at-length) during three ecologically distinct time periods. Condition of both species varied among time periods, with smallmouth bass experiencing greater proportional changes than northern pike. Smallmouth bass weight-at-length was greatest during 2005\textendash 2013 and lowest during 1982\textendash 1992. Northern pike weight-at-length was greatest during 2005\textendash 2013 for quantiles {$\geq$}0.5, similar between the 1982\textendash 1992 and 2005\textendash 2013 time periods for lower quantiles, and lowest during 1993\textendash 2004. The most likely weight\textendash length model for smallmouth bass included terms for length, presence of round goby (Neogobius melanostomus), presence of dreissenid mussels, and an interaction between length and dreissenid mussels (wi = 0.93). The most likely model for northern pike included terms for length, presence of round goby, presence of dreissenid mussels, water temperature, conspecific abundance, and sex (wi = 0.56). Invasive species and environmental change related to invasive species have altered the condition of smallmouth bass and northern pike in the St. Lawrence River.},
  file = {/Users/georgepv/Zotero/storage/45SLPRWF/Crane&Farrell.2017.pdf;/Users/georgepv/Zotero/storage/TEWB7RVZ/Crane&Farrell.2017.pdf;/Users/georgepv/Zotero/storage/YKZWIHD9/Crane&Farrell.2017.pdf}
}

