@article{bjornstad2001,
  title = {Nonparametric Spatial Covariance Functions: {{Estimation}} and Testing},
  author = {Bj{\o}rnstad, Ottar N. and Falck, Wilhelm},
  year = {2001},
  journal = {Environmental and Ecological Statistics},
  volume = {8},
  number = {1},
  pages = {53--70},
  doi = {10.1023/A:1009601932481},
  abstract = {Spatial autocorrelation techniques are commonly used to describe genetic and ecological patterns. To improve statistical inference about spatial covariance, we propose a continuous nonparametric estimator of the covariance function in place of the spatial correlogram. The spline correlogram is an adaptation of a recent development in spatial statistics and is a generalization of the commonly used correlogram. We propose a bootstrap algorithm to erect a confidence envelope around the entire covariance function. The meaning of this envelope is discussed. Not all functions that can be drawn inside the envelope are candidate covariance functions, as they may not be positive semidefinite. However, covariance functions that do not fit, are not supported by the data. A direct estimate of the L0 spatial correlation length with associated confidence interval is offered and its interpretation is discussed. The spline correlogram is found to have high precision when applied to synthetic data. For illustration, the method is applied to electrophoretic data of an alpine grass (Pou alpina).},
  keywords = {Bootstrapping dependent data,Correlogram,Geostatistis,Nonparametric regression,Population genetics,Smoothing spline,Spatial autocorrelation},
  file = {/Users/georgepv/Zotero/storage/53C5SR5C/BjØrnstad-Falck2001_Article_NonparametricSpatialCovariance.pdf;/Users/georgepv/Zotero/storage/9IZMJWW8/BjØrnstad-Falck2001_Article_NonparametricSpatialCovariance.pdf;/Users/georgepv/Zotero/storage/DM5ETEDR/BjØrnstad-Falck2001_Article_NonparametricSpatialCovariance.pdf;/Users/georgepv/Zotero/storage/JZ5XFRM6/BjØrnstad-Falck2001_Article_NonparametricSpatialCovariance.pdf;/Users/georgepv/Zotero/storage/KUL8G4EP/BjØrnstad-Falck2001_Article_NonparametricSpatialCovariance.pdf;/Users/georgepv/Zotero/storage/P3JACWT4/BjØrnstad-Falck2001_Article_NonparametricSpatialCovariance.pdf;/Users/georgepv/Zotero/storage/P4D8VSFH/BjØrnstad-Falck2001_Article_NonparametricSpatialCovariance.pdf;/Users/georgepv/Zotero/storage/PAWH4KX5/BjØrnstad-Falck2001_Article_NonparametricSpatialCovariance.pdf;/Users/georgepv/Zotero/storage/W64BDDSN/BjØrnstad-Falck2001_Article_NonparametricSpatialCovariance.pdf;/Users/georgepv/Zotero/storage/Y7QVXJLJ/BjØrnstad-Falck2001_Article_NonparametricSpatialCovariance.pdf}
