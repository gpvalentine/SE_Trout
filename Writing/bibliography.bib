@article{soranno2014,
  title = {Cross-Scale Interactions: Quantifying Multi-Scaled Cause\textendash Effect Relationships in Macrosystems},
  shorttitle = {Cross-Scale Interactions},
  author = {Soranno, Patricia A and Cheruvelil, Kendra S and Bissell, Edward G and Bremigan, Mary T and Downing, John A and Fergus, Carol E and Filstrup, Christopher T and Henry, Emily N and Lottig, Noah R and Stanley, Emily H and Stow, Craig A and Tan, Pang-Ning and Wagner, Tyler and Webster, Katherine E},
  year = {2014},
  journal = {Frontiers in Ecology and the Environment},
  volume = {12},
  number = {1},
  pages = {65--73},
  issn = {1540-9309},
  doi = {10.1890/120366},
  abstract = {Ecologists are increasingly discovering that ecological processes are made up of components that are multi-scaled in space and time. Some of the most complex of these processes are cross-scale interactions (CSIs), which occur when components interact across scales. When undetected, such interactions may cause errors in extrapolation from one region to another. CSIs, particularly those that include a regional scaled component, have not been systematically investigated or even reported because of the challenges of acquiring data at sufficiently broad spatial extents. We present an approach for quantifying CSIs and apply it to a case study investigating one such interaction, between local and regional scaled land-use drivers of lake phosphorus. Ultimately, our approach for investigating CSIs can serve as a basis for efforts to understand a wide variety of multi-scaled problems such as climate change, land-use/land-cover change, and invasive species.},
  langid = {english},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1890/120366},
  file = {/Users/georgepv/Zotero/storage/7PUZAC34/Soranno et al. - 2014 - Cross-scale interactions quantifying multi-scaled.pdf;/Users/georgepv/Zotero/storage/CCI9FSIT/120366.html}
}

