  doi = {10.1139/z95-217},
  abstract = {Aggressive interactions, foraging behaviour, microhabitats, and growth of individuals of white-spotted charr Salvelinus leucomaenis and masu salmon Oncorhynchus masou masou, were studied in a mountain stream in central Japan. The fishes within a single pool in the stream formed an interspecific size-structured dominance hierarchy. Dominant individuals of both species maintained foraging territories against both subordinate conspecifics and heterospecifics, whereas subordinate charr adopted nonterritorial tactics. In each species, the most dominant fish usually held the focal point nearest to the pool inlet, which ensured priority of access to drifting food, with subordinates distributed farther downstream. Foraging microhabitats differed vertically in the water column between the species, charr utilizing a home range near the stream bed and salmon occupying the midlayer. The frequency of foraging attempts by salmon was considerably higher than that by char. Although daily body mass increments of dominant individuals were significantly larger than those of conspecific subordinates in each species, those of some subordinate salmon exceeded those of dominant charr. -Author},
  file = {/Users/georgepv/Zotero/storage/42ZZQV9F/Nakano 1995 Can J Zoology.pdf;/Users/georgepv/Zotero/storage/5MWPA5CF/Nakano 1995 Can J Zoology.pdf;/Users/georgepv/Zotero/storage/GH7QTW9R/Nakano 1995 Can J Zoology.pdf}
}

@article{nakano1995a,
  title = {Individual {{Differences}} in {{Resource Use}}, {{Growth}} and {{Emigration Under}} the {{Influence}} of a {{Dominance Hierarchy}} in {{Fluvial Red-Spotted Masu Salmon}} in a {{Natural Habitat}}},
  author = {Nakano, Shigeru},
  year = {1995},
  month = jan,
  journal = {The Journal of Animal Ecology},
  volume = {64},
  number = {1},
  pages = {75--75},
  publisher = {{JSTOR}},
  doi = {10.2307/5828},
  abstract = {1. The relationships between dominance status and individual\textbackslash ndifferences in foraging behaviour, habitat use, growth and emigration\textbackslash nwere examined for fluvial red-spotted masu salmon, Oncorhynchus masou\textbackslash nishikawai, in a mountain stream.\textbackslash n2. Size-structured linear dominance hierarchies were recognized among\textbackslash nindividuals inhabiting the same stream pools. Observations on space\textbackslash nutilization and foraging behaviour revealed fish to be either\textbackslash nterritorial or nonterritorial. Within each local pool, dominant fish\textbackslash nexclusively occupied the mid or surface layer of the pools as foraging\textbackslash nterritories, whereas subordinates adopted nonterritorial tactics,\textbackslash nprimarily utilizing the bottom layer.\textbackslash n3. Of the territorial fish, more dominant individuals tended to occupy\textbackslash nfocal points nearer the pool inlet, where they had priority of access to\textbackslash ndrifting food items. These fish showed higher actual foraging rates,\textbackslash nfeeding upon larger prey than their subordinates. This foraging\textbackslash nadvantage resulted in their having larger daily growth increments.\textbackslash n4. The more dominant fish in each pool exhibited a more sedentary\textbackslash ntendency than their subordinates. Population densities in the pools did\textbackslash nnot fluctuate appreciably owing to both emigration of nonterritorial\textbackslash nsubordinates and immigration.\textbackslash n5. These results support the hypothesis that unequal resource\textbackslash npartitioning among individuals subject to a dominance hierarchy plays an\textbackslash nimportant role in their density-dependent population regulation.}
}

