@incollection{fletcher2018a,
  title = {Spatial {{Dependence}} and {{Autocorrelation}}},
  booktitle = {Spatial {{Ecology}} and {{Conservation Modeling}}: {{Applications}} with {{R}}},
  author = {Fletcher, Robert and Fortin, Marie-Jos{\'e}e},
